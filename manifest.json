{
  "manifest_version": 2,

  "name": "url-iterator",
  "description": "Iterating URL google extension.",
  "version": "0.3.0",

  "permissions": [
    "tabs"
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "NextURL": {
      "suggested_key": {
        "default": "Alt+Shift+S"
      },
      "description": "Next URL"
    },
    "PrevURL": {
      "suggested_key": {
        "default": "Alt+Shift+A"
      },
      "description": "Prev URL"
    }
  }
}